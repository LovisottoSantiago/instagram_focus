{
    "manifest_version": 2,
    "name": "Instagram Focus",
    "version": "1.0",
    "description": "Oculta elementos de Instagram para mayor concentraci√≥n (reels, inicio, etc.).",
    "permissions": ["tabs", "activeTab"],
    "content_scripts": [
      {
        "matches": ["*://www.instagram.com/*"],
